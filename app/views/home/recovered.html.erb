<h1>Recuperados</h1>

<table>
  <thead>
    <td>País</td>
    <td>Cidade</td>
    <td>Recuperados</td>
    <td>Última atualização</td>

    <tbody>
      <% @recovered.each do |recovered|%>
        <tr>
          <td><%= recovered['countryRegion']%></td>
          <td><%= recovered['provinceState']%></td>
          <td><%= recovered['recovered']%></td>
          <td><%= Time.at(recovered['lastUpdate'].to_i/1000)
                      .strftime('%d/%m/%Y %H:%M:%S')%></td>
        </tr>
      <% end %>
    </tbody>
  </thead>
</table>